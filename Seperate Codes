package runner;

import java.io.IOException;
import java.net.MalformedURLException;

import org.openqa.selenium.By;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

import utils.Base;
import utils.Screenshot;
import utils.WebDriverHelper;

public class TestSample extends Base {

    ExtentReports reports;
    ExtentSparkReporter spark;
    @BeforeClass
    public void getReports(){
        reports = new ExtentReports();
        spark = new ExtentSparkReporter("reports/myReport.html");
        spark.config().setTheme(Theme.DARK);
        spark.config().setDocumentTitle("Orange HRM report");

        if(reports != null) {
            reports.attachReporter(spark);
        }

   
    }

    WebDriverHelper helper;
    Screenshot screenshot;
    @BeforeMethod
    public void launchBrowser() throws MalformedURLException, InterruptedException {
        // launch your browser
        openBrowser();
        driver.get("https://www.tatacliq.com./");
        Thread.sleep(10000);
    }

    @Test
    public void testOne() throws InterruptedException, IOException {
        // write or call ur pages here
           driver.findElement(By.xpath("//button[@id='moe-dontallow_button']")).click();
           By ele=By.xpath("//*[@id='root']/div/div[4]/div[2]/div[1]/div[1]/div");
           helper.javascriptScroll(ele);
           driver.findElement(By.xpath("//*[@id='root']/div/div[4]/div[2]/div[1]/div[2]/a[9]]")).click();

          Thread.sleep(5000);

          
          Screenshot.getScreenShot(driver, "screenshot");
           
    }

    @AfterMethod
    public void tearDown() {
        // quit the driver here
        driver.quit();
    }
     @AfterClass
    public void finalReports(){
       
            reports.flush();
    }
}


package runner;

import java.io.IOException;
import java.net.MalformedURLException;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.testng.annotations.AfterClass;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.Test;

import com.aventstack.extentreports.ExtentReports;

import utils.Base;
import utils.LoggerHandler;
import utils.Reporter;
import utils.Screenshot;
import utils.WebDriverHelper;

public class TestRunner extends Base {
     WebDriverHelper helper;
     ExtentReports reports;
     Reporter reporter;
     @BeforeClass
     public void generatereport()
     {
        reports=reporter.generateExtentReport("Report");
     }
    @BeforeMethod
    public void launchBrowser() throws MalformedURLException {
        openBrowser();
        helper=new WebDriverHelper(driver);
    }

    @Test
    public void testOne() throws InterruptedException, IOException {
        // write or call ur pages here
        Thread.sleep(5000);
        By ele=By.xpath("//a[text()=' BMW ConnectedDrive Upgrades ']");
        helper.javascriptScroll(ele);
        Thread.sleep(5000);
        driver.findElement(By.xpath("//a[text()=' BMW ConnectedDrive Upgrades ']")).click();
        helper.switchToNewWindow();
      By ele1=By.xpath("//ul[@class='tw-grid tw-auto-rows-[_auto_auto_auto_auto] tw-gap-x-6 tw-gap-y-0 tw-grid-cols-1 md:tw-grid-cols-2 m1:tw-grid-cols-3 lg:tw-grid-cols-4 xl:tw-grid-cols-6']//li[4]");
      helper.hoverOverElement(ele1);
      driver.findElement(By.xpath("//ul[@class='tw-grid tw-auto-rows-[_auto_auto_auto_auto] tw-gap-x-6 tw-gap-y-0 tw-grid-cols-1 md:tw-grid-cols-2 m1:tw-grid-cols-3 lg:tw-grid-cols-4 xl:tw-grid-cols-6']//li[4]")).click();
      Thread.sleep(2000);
      Screenshot.getScreenShot(driver, "screenshot");
      LoggerHandler.logInfo("testcase done");



        
    }

    @AfterMethod
    public void tearDown() {
        // quit the driver here
        driver.quit();
    }
    @AfterClass
    public void flushreport()
    {
        reports.flush();
    }
}
