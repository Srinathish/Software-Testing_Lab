       driver=openBrowser();
       helper=new WebDriverHelper(driver);
       driver.get("https://codescandy.com/");
    }

    @Test
    public void Test() throws InterruptedException, IOException
    {
        Thread.sleep(3000);
    By ele= By.xpath("//input[@name='email']");
     helper.javascriptScroll(ele);
     Thread.sleep(5000);
    driver.findElement(By.xpath("//input[@name='email']")).sendKeys("admin@gmail.com");
    driver.findElement(By.xpath("//*[@id='gumroad-follow-form-embed-button']/span/span")).click();

